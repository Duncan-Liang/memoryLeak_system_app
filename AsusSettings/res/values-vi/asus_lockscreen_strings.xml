    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"Thêm vân tay"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"Sử dụng vân tay để mở khóa thiết bị hoặc truy cập cuộc gọi và dữ liệu của điện thoại của bạn. Bạn sẽ cần thiết lập phương thức khóa màn hình cùng với vân tay."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"Định vị máy quét vân tay"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Tìm cảm biến vân tay ở mặt trước thiết bị của bạn. Điều chỉnh ngón tay của bạn và thử che khuất các phần khác nhau của ngón tay bạn."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Tìm cảm biến vân tay ở mặt sau thiết bị của bạn. Điều chỉnh ngón tay và thử che khuất các phần khác nhau của ngón tay."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"Đặt và nhấc lên"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"Đặt ngón tay của bạn trên cảm biến vân tay và nhấc lên khi bạn cảm thấy rung."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"Giờ hãy lặp lại"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"Lặp lại thao tác đặt và nhắc lên cho đến khi chụp được ảnh toàn bộ vân tay."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"Chụp thêm"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"Điều chỉnh việc cầm nắm của bạn và đăng ký nhiều phần khác nhau của ngón tay ."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[Nhấn nút quay lại để quay trở về. Nút Màn hình chính bị vô hiệu trong quá trình đăng ký .]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
