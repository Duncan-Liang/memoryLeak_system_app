    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"Ujjlenyomat hozzáadása"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"Használjon ujjlenyomatot a készülék feloldásához, illetve a telefonhívások és az adatok használatához. Az ujjlenyomat mellett ehhez képernyő-lezárási módot is be kell állítania."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"Keresse meg az ujjlenyomat-érzékelőt"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Keresse meg a készülék előlapján található ujjlenyomat-érzékelőt. Igazítsa oda az ujját, próbálja az ujja különböző részeit beolvasni."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Keresse meg a készülék hátlapján található ujjlenyomat-érzékelőt. Igazítsa oda az ujját, próbálja az ujja különböző részeit beolvasni."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"Tegye rá és emelje fel"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"Helyezze az ujját az ujjlenyomat-érzékelőre, és ha rezgést érez, emelje fel."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"Most ismételje meg"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"Ismételje meg ezt az odahelyez-felemel műveletet, amíg a rendszer a teljes ujjlenyomatát nem rögzíti."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"Rögzítsen több részletet"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"Fogja máshogyan a készüléket, és rögzítse az ujja különböző részleteit."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[A visszatéréshez nyomja meg a Vissza gombot. A rögzítés során a Kezdőlap gomb le van tiltva.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"Kész"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"Az ujjlenyomat kész. Bármely tárolt ujjlenyomat használható a készülék feloldására, hívásfogadáshoz, illetve az adatok elérésére."</string>
