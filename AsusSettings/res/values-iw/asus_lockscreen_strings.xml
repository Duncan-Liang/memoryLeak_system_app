    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"הוסף טביעת אצבע"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"השתמש בטביעות אצבע לשחרור ההתקן או כדי לגשת לשיחות הטלפון ולנתונים. עליך להגדיר שיטת נעילת מסך יחד עם טביעת אצבע."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"מצא את סורק טביעות האצבע"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"מצא את סורק טביעות האצבע בחזית של ההתקן. התאם את האצבע לסורק ונסה ללכוד חלקים שונים של האצבע בכל הנחה."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"מצא את סורק טביעות האצבע בגב ההתקן. התאם את האצבע לסורק ונסה ללכוד חלקים שונים של האצבע בכל הנחה."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"הנח והרם"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"הנח אצבע על סורק טביעות האצבע והרם אותה כשתרגיש ברטט."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"חזור שנית"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"בצע שנית את פעולת ההנחה וההרמה עד השלמת הלכידה של טביעת אצבע מלאה."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"לכוד עוד"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"שנה את ההצבה כדי לרשום חלק אחר של האצבע."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[לחץ על מקש Back כדי לחזור. לחצן \"בית\" מושבת במהלך ההרכשה.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"הושלם"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"טביעת האצבע מוכנה. כל טביעת אצבע מאוחסנת עשויה לשמש לשחרור הנעילה של ההתקן, של תשובה לשיחה ושל הנתונים."</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"נכשל"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"ההרשמה של טביעת האצבע נכשלה. נסה שנית מאוחר יותר."</string>
    <string name="asus_fingerprint_acquied_too_similar">"שנה את ההצבה של האצבע כדי לסרוק חלקים אחרים של טביעת האצבע."</string>
    <string name="asus_security_settings_fingerprint_enroll_lift_touch_again">"הנח והרם את האצבע, פעם אחר פעם, על סורק טביעות האצבע"</string>
    <!-- Text shown in fingerprint settings explaining what the fingerprint can be used for in the case unlocking is disabled [CHAR LIMIT=NONE] -->
