    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"Fingerabdruck hinzufügen"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"Verwenden Sie den Fingerabdruck, um das Gerät zu entsperren oder auf Ihre Telefonanrufe und Daten zuzugreifen. Sie müssen eine Bildschirm-Sperrmethode mit dem Fingerabdruck einrichten."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"Suchen des Fingerabdruckscanners"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Sie finden den Fingerabdrucksensor vorn an Ihrem Gerät. Positionieren Sie Ihren Finger und versuchen Sie, verschiedene Bereiche Ihres Fingers abzudecken."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Sie finden den Fingerabdrucksensor hinten an Ihrem Gerät. Positionieren Sie Ihren Finger und versuchen Sie, verschiedene Bereiche Ihres Fingers abzudecken."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"Auflegen und anheben"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"Legen Sie Ihren Finger auf den Fingerabdrucksensor und heben Sie ihn einmal ab, wenn Sie die Vibration spüren."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"Jetzt wiederholen"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"Wiederholen Sie dieses Ablegen und Anheben, bis der gesamte Fingerabdruck erfasst ist."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"Mehr aufnehmen"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"Passen Sie Ihren Griff an und registrieren Sie unterschiedliche Bereiche Ihres Fingers."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[Drücken Sie auf die Rücktaste, um zurückzukehren. Die Starttaste ist während der Registrierung deaktiviert.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"Abgeschlossen"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"Ihr Fingerabdruck ist bereit. Alle gespeicherten Fingerabdrücke können zum Entsperren des Gerätes, zum Annehmen von Anrufen und zum Aufrufen von Daten verwendet werden."</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"Fehlgeschlagen"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"Fingerabdruckregistrierung fehlgeschlagen. Bitte versuchen Sie es später erneut."</string>
    <string name="asus_fingerprint_acquied_too_similar">"Verschieben und passen Sie Ihren Finger leicht an, um unterschiedliche Bereiche Ihres Fingerabdrucks zu scannen."</string>
    <string name="asus_security_settings_fingerprint_enroll_lift_touch_again">"Legen Sie wiederholt Ihren Finger auf den Fingerabdruckscanner und heben Sie ihn an"</string>
    <!-- Text shown in fingerprint settings explaining what the fingerprint can be used for in the case unlocking is disabled [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_disabled_on_lockscreen">"Option für Bildschirmsperre deaktiviert"</string>
