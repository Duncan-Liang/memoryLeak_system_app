    <string name="security_settings_manage_fingerprint_preference_title">"Fingerabdrücke verwalten"</string>
    <string name="secure_with_fingerprint_category">"Fingerabdruck verwenden für"</string>
    <string name="secure_unlock_with_fingerprint_item">"Gerät entsperren"</string>
    <string name="secure_unlock_with_fingerprint_summary">"Fingerabdruck zum Entsperren des Bildschirms verwenden."</string>
