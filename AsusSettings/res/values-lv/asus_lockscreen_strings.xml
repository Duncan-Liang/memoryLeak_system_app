    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"Pievienot pirkstu nospiedumu"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"Izmantojiet pirkstu nospiedumus, lai atbloķētu ierīci vai piekļūtu tālruņa zvaniem un datiem. Nepieciešams iestatīt ekrāna bloķēšanas metodi ar pirkstu nospiedumu."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"Atrast pirkstu nospiedumu skeneri"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Ierīces priekšpusē atrodiet pirkstu nospiedumu sensoru. Pielāgojiet pirkstu nospiedumu un mēģiniet nosegt citu pirkstu nospieduma daļu."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Ierīces aizmugurē atrodiet pirkstu nospiedumu sensoru. Pielāgojiet pirkstu nospiedumu un mēģiniet nosegt citu pirkstu nospieduma daļu."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"Uzlieciet un paceliet"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"Nolieciet pirkstu uz pirkstu nospiedumu sensora un paceliet to nost, tiklīdz notiek vibrācija."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"Atkārtot"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"Atkārtojiet šo nolikšanu un pacelšanu, līdz ir tverts viss pirksta nospiedums."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"Tvert vairāk"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"Pielāgojiet tvērienu un reģistrējiet dažādas pirksta pozīcijas."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[Nospiediet pogu Atpakaļ, lai atgrieztos. Reģistrēšanas brīdī sākuma poga ir atspējota.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"Pabeigt"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"Pirksta nospiedums ir gatavs. Jebkuru saglabāto pirkstu nospiedumu var izmantot, lai atbloķētu ierīci, atbildētu uz zvanu un piekļūtu datiem."</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"Neizdevās"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"Pirksta nospieduma reģistrēšana neizdevās. Vēlāk mēģiniet vēlreiz."</string>
    <string name="asus_fingerprint_acquied_too_similar">"Nedaudz pārvietojiet un pielāgojiet pirkstu, lai skenētu dažādas pirksta nospieduma zonas"</string>
