    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"Pridėti piršto atspaudą"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"Piršto atspaudu atrakinkite įrenginį arba naudokitės telefono skambučiais ir duomenimis. Reikės nustatyti ekrano užrakinimo metodą ir pateikti piršto atspaudą."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"Suraskite piršto atspaudų skaitytuvą"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Suraskite piršto atspaudų jutiklį, esantį įrenginio priešakyje. Uždėkite pirštą ir pabandykite uždengti skirtingomis piršto dalimis."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Suraskite piršto atspaudų jutiklį, esantį įrenginio gale. Uždėkite pirštą ir pabandykite uždengti skirtingomis piršto dalimis."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"Uždėkite ir pakelkite"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"Uždėkite pirštą ant pirštų atspaudų jutiklio ir iš karto pakelkite, kai pajusite vibraciją."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"Dabar kartoti"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"Kartokite šį uždėjimo ir pakėlimo veiksmą, kol bus užfiksuotas visas piršto atspaudas."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"Fiksuoti daugiau"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"Atitinkamai suspauskite ir užregistruokite įvairias piršto vietas."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[Paspauskite mygtuką „Atgal“ norėdami sugrįžti. Per įtraukimo procesą pradžios mygtukas yra išjungtas.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"Baigta"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"Jūsų piršto atspaudas paruoštas. Bet kokiu išsaugotu piršto atspaudu galima atrakinti įrenginį, atsiliepti į skambučius ir naudotis duomenimis."</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"Nepavyksta"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"Nepavyko užregistruoti pirštų atspaudo. Bandykite vėliau dar kartą."</string>
    <string name="asus_fingerprint_acquied_too_similar">"Šiek tiek pajudinkite ir pakreipkite pirštą, kad būtų nuskaitytos įvairios piršto atspaudo sritys."</string>
    <string name="asus_security_settings_fingerprint_enroll_lift_touch_again">"Kelis kartus uždėkite pirštą ant pirštų atspaudų skaitytuvo ir nukelkite"</string>
    <!-- Text shown in fingerprint settings explaining what the fingerprint can be used for in the case unlocking is disabled [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_disabled_on_lockscreen">"Ekrano užrakinimo parinktis išjungta"</string>
