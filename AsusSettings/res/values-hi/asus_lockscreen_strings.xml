    <string name="security_settings_manage_fingerprint_preference_title">"फ़िगरप्रिंट प्रबंधित करें"</string>
    <string name="secure_with_fingerprint_category">"फ़िंगरप्रिंट का उपयोग इसके लिए करें"</string>
    <string name="secure_unlock_with_fingerprint_item">"डिवाइस अनलॉक करें"</string>
    <string name="secure_unlock_with_fingerprint_summary">"स्क्रीन अनलॉक करने के लिए फ़िंगरप्रिंट का उपयोग करें।"</string>
    <string name="fingerprint_management_category">"संगृहीत फ़िंगरप्रिंट"</string>
    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"फ़िंगरप्रिंट जोड़ें"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"डिवाइस अनलॉक करने या अपनी फ़ोन कॉल और डेटा तक पहुँच करने के लिए फ़िंगरप्रिंट का उपयोग करें। आपको फ़िंगरप्रिंट के साथ स्क्रीन लॉक विधि सेट करनी होगी।"</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"फ़िंगरप्रिंट सेंसर की स्थिति जानें"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"फ़िंगरप्रिंट सेंसर अपनी डिवाइस के सामने देखें। अपनी उँगली समायोजित करें और अपनी उँगली के विभिन्न भाग कवर करने का प्रयास करें।"</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"फ़िंगरप्रिंट सेंसर अपनी डिवाइस के पीछे देखें। अपनी उँगली समायोजित करें और अपनी उँगली के विभिन्न भाग कवर करने का प्रयास करें।"</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"रखें और उठाएँ"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"फ़िंगरप्रिंट सेंसर पर अपनी उँगली रखें और जब आप कंपन महसूस करें तो इसे उठा लें।"</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"अब दोहराएँ"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"इस रखें-और-उठाएँ कार्रवाई को तब तक दोहराएँ जब तक पूरा फ़िंगरप्रिंट कैप्चर न कर लिया जाए।"</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"और कैप्चर करें"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"अपनी ग्रिप समायोजित करें और अपनी उँगली के विभिन्न भाग पंजीकृत करें।"</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[वापस लौटने के लिए वापस कुंजी दबाएँ। नामांकन के दौरान होम बटन अक्षम कर दिया जाता है।]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"पूर्ण"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"आपका फ़िंगरप्रिंट तैयार है। किसी संगृहीत फ़िंगरप्रिंट का इस्तेमाल डिवाइस अनलॉक करने, कॉल का उत्तर देने, और डेटा तक पहुँच करने के लिए किया जा सकता है।"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"विफल"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"फ़िंगरप्रिंट पंजीकरण विफल। कृपया बाद में पुनः प्रयास करें।"</string>
    <string name="asus_fingerprint_acquied_too_similar">"अपने फ़िंगरप्रिंट के विभिन्न क्षेत्र स्कैन करने के लिए अपनी उँगली को थोड़ा हिलाएँ और समायोजित करें।"</string>
    <string name="asus_security_settings_fingerprint_enroll_lift_touch_again">"अपनी उँगली फ़िंगरप्रिंट सेंसर पर बार-बार रखें, और फिर उठाएँ"</string>
