    <!-- Introduction title shown in fingerprint enrollment to introduce the fingerprint feature[CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_introduction_title">"افزودن اثر انگشت"</string>
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="asus_security_settings_fingerprint_enroll_introduction_message">"برای باز کردن قفل دستگاه یا دسترسی به تماس ها و داده های تلفن، از اثر انگشت استفاده نمایید. لازم است روشی را برای قفل صفحه به همراه اثر انگشت تعیین کنید."</string>
    <!-- Introduction title shown in fingerprint enrollment dialog to locate the sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_title">"تعیین محل اسکنر اثر انگشت"</string>
    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"حسگر اثر انگشت را در جلوی دستگاه بیابید. انگشت خود را تنظیم کنید و سعی کنید انگشت شما به طور کامل روی حسگر قرار گیرد."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"حسگر اثر انگشت را در عقب دستگاه بیابید. انگشت خود را تنظیم کنید و سعی کنید انگشت شما به طور کامل روی حسگر قرار گیرد."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
    <string name="asus_security_settings_fingerprint_enroll_start_title">"قرار دهید و بردارید"</string>
    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_start_message">"انگشتتان را روی حسگر اثر انگشت قرار دهید و زمانی که لرزش را احساس کردید، آن را بردارید."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_title">"تکرار کنید"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_repeat_message">"این عمل گذاشتن و برداشتن را تکرار کنید تا اینکه کل اثر انگشتتان ثبت شود."</string>
    <!-- Title shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_more_title">"ثبت اثر انگشتان دیگر"</string>
    <!-- Message shown in fingerprint enrollment dialog to repeat touching the fingerprint sensor [CHAR LIMIT=NONE] -->
    <string name="asus_security_settings_fingerprint_enroll_more_message">"جای دستتان را تنظیم کنید و قسمت های مختلفی از انگشتتان را ثبت نمایید."</string>
    <string name="asus_security_settings_fingerprint_enroll_hint">"[برای بازگشت، دکمه بازگشت را فشار دهید. دکمه خانه در حین عملیات ثبت غیرفعال می شود.]"</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is completed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_finish_title">"کامل"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is completed -->
    <string name="asus_security_settings_fingerprint_enroll_finish_message">"اثر انگشت شما آماده است. برای باز کردن قفل دستگاه، پاسخ دادن به تماس ها و دسترسی به داده ها، از هر اثر انگشتی که ذخیره شده باشد می توان استفاده کرد."</string>
    <!-- Title shown in fingerprint enrollment dialog once enrollment is failed [CHAR LIMIT=29] -->
    <string name="asus_security_settings_fingerprint_enroll_fail_title">"ناموفق بود"</string>
    <!-- Message shown in fingerprint enrollment dialog once enrollment is failed -->
    <string name="asus_security_settings_fingerprint_enroll_fail_message">"ثبت اثر انگشت ناموفق بود. بعداً دوباره تلاش کنید."</string>
    <string name="asus_fingerprint_acquied_too_similar">"برای اسکن قسمتهای مختلف اثر انگشت، انگشت خود را حرکت داده و تنظیم کنید"</string>
    <string name="asus_security_settings_fingerprint_enroll_lift_touch_again">"انگشتتان را چندبار روی اسکنر اثر انگشت قرار دهید و بردارید"</string>
