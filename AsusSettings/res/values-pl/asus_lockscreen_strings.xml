    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_front">"Znajdź czujnik odcisków palców z przodu urządzenia. Przyłóż palec i spróbuj zeskanować różne fragmenty palca."</string>
    <string name="asus_security_settings_fingerprint_enroll_find_sensor_message_back">"Znajdź czujnik odcisków palców z tyłu urządzenia. Przyłóż palec i spróbuj zeskanować różne fragmenty palca."</string>
    <!-- Title shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=29]-->
