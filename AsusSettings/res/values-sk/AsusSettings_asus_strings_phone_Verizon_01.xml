    <string name="vzw_data_usage_limit_reached_notification_message">"Dotykom na túto ikonu ho zapnite"</string>
    <string name="vzw_data_usage_limit_exceeded_notification_title">"Prekročený limit mobilného dátového pripojenia"</string>
