    <string name="vzw_data_usage_limit_exceeded_notification_title">"Перевищено ліміт мобільних даних"</string>
    <string name="vzw_data_usage_limit_exceeded_notification_message">"<xliff:g example="10.52" id="exceeded_value">%s</xliff:g> <xliff:g example="MB" id="exceeded_unit">%s</xliff:g> понад визначений ліміт"</string>
