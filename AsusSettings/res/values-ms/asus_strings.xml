    <string name="user_snapview_settings_change_info">Ubah maklumat profil</string>
    <string name="user_snapview_settings_delete_self">Padam Mod swasta</string>
    <string name="user_snapview_settings_item1">Tidak Ketara</string>
    <string name="user_snapview_settings_item2">Ketara</string>
    <string name="user_snapview_settings_item3">Tiada</string>
    <string name="user_snapview_settings_item_color">Warna</string>
